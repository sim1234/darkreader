{
    "commonSelectors": "img, iframe, video, *:not(object):not(body)>embed, object, *[style*=\"background:url\"]:empty, *[style*=\"background-image:url\"]:empty, *[style*=\"background: url\"]:empty, *[style*=\"background-image: url\"]:empty",
    "specials": [
        {
            "url": "basho.com",
            "selectors": "iframe, video, embed, object, #site-navigation, footer, header, .featured-news-wrapper, *:not(.lang):not(#foot-logo)>img"
        },
        {
            "url": "facebook.com",
            "selectors": "{common}, ._3ixn, ._2teu, .uiStreamStory",
            "rules": "._4d3w .stageWrapper { background: white; } .uiStreamStory video { -webkit-filter: none !important; }"
        },
        {
            "url": "gigaom.com",
            "selectors": "{common}, .bg"
        },
        {
            "url": "google.*",
            "selectors": "img:not(.irc_mi):not(.irc_rii), iframe, video, embed, .gbii, .gbip, .azp, .aLF-aPX-KP, .irc_bg, .RY3tic"
        },
        {
            "url": "google.*.*",
            "selectors": "img:not(.irc_mi):not(.irc_rii), iframe, video, embed, .gbii, .gbip, .azp, .aLF-aPX-KP, .irc_bg, .RY3tic"
        },
        {
            "url": "granttree.co.uk",
            "selectors": "img, iframe, video, embed, .photo"
        },
        {
            "url": "ign.com",
            "selectors": "{common}, .video_embed_content-poster, .video_embed_content-poster-play-button",
            "rules": "#review-promo { background-color: white; }"
        },
        {
            "url": "inpad.com.tw",
            "selectors": "{common}, *[style*=\"background-image:url\"]"
        },
        {
            "url": "io9.com",
            "selectors": "{common}",
            "rules": "html { height: auto !important; }"
        },
        {
            "url": "lifehacker.com",
            "selectors": "{common}, .videoCube a .thumbBlock",
            "rules": "html { height: auto !important; }"
        },
        {
            "url": "mail.live.com",
            "selectors": ".ComposeContent { background-color:white; } img:not([src=\"https://a.gfx.ms/rte_metro2.png\"]), video, object, .ComposeContent",
            "rules": ".ComposeContent { background-color: white; }"
        },
        {
            "url": "shop.ubi.com",
            "selectors": "{common} #topNavbar, #UplayHeader"
        },
        {
            "url": "spotify.com",
            "selectors": "img, iframe, video, embed, .hero, .header-main, .search-bar, #bg-wrap, #main"
        },
        {
            "url": "twitch.tv",
            "selectors": "{common}, #site_footer, #carousel_background:after, #left_col, #player",
            "rules": "html, .nav, #carousel, .dark_wrapper ul { background-color: white !important; } .dark_wrapper * { color:black !important; } #player * { -webkit-filter: none !important; }"
        },
        {
            "url": "ubi.com",
            "selectors": "{common}",
            "rules": "html, body { background: white !important; }"
        },
        {
            "url": "vk.com",
            "selectors": "img, iframe, video, embed, .video_image_div, #layer_bg"
        },
        {
            "url": "wikipedia.org",
            "selectors": "img:not(.mwe-math-fallback-image-inline), iframe, video, embed"
        },
        {
            "url": "youtube.com",
            "selectors": "iframe, .html5-video-player, .has-custom-banner, #theater-background, #watch-appbar-playlist, *:not(.html5-video-player):not(.channel-header-profile-image-container):not(.html5-storyboard-filmstrip):not(.html5-storyboard-framepreview):not(.html5-storyboard-lens) > img"
        }
    ]
}
